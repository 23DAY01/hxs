<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lypc.hxs.mapper.ContactMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lypc.hxs.pojo.domain.Contact">
        <id column="contact_id" property="contactId" />
        <result column="team_phone" property="teamPhone" />
        <result column="team_wechat" property="teamWechat" />
        <result column="team_name" property="teamName" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        contact_id, team_phone, team_wechat, team_name, create_time, update_time, deleted
    </sql>

</mapper>
